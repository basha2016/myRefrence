package com.example1; public class Multiplication { public int multiply(String numbers) { int result = 1; if(numbers.isEmpty()) { result = 0; } else { try { int sum= 1; String[] numbersArray = numbers.split(","); for (String number : numbersArray) { if(!number.isEmpty()) { sum = sum * Integer.parseInt(number); result = sum; } else { throw new NumberFormatException("Empty Numbers cannot be multiplied"); } } } catch(NumberFormatException e) { throw e; } } return result; } }
